<div class="row">
  {{#each Notes}}
  <div class="col s12">
    <div class="card">
      <div class="card-title teal">
        {{title}}
      </div>
      <div class="card-content">
        <div class="container">
          <p>
          {{description}}
          </p>
        </div>
      </div>
      <div class="card-action">
        <p>Author: {{user.firstName}} {{user.lastName}}</p>
        <a href="/notes/detail/{{_id}}">Read More....</a>
      </div>
    </div>
  </div>
  {{/each}}

</div>